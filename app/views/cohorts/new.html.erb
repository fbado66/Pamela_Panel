<div class="container">  
   <div class="page-header">
      <h1>Cohorts#new</h1>
         <p>Find me in app/views/cohorts/new.html.erb</p>
   </div>
<div class="col-6 offset-md-3">
 <div class="jumbotron">
   <div class="col-md-3"> 
    <div class="form-group">
    <%= form_with(model: @cohort) do |form| %>
     <strong>Name:</strong> <%= form.text_field(:name) %> <br /><br />
     <div> <strong>Start On:</strong> </div>
     <%= form.date_field(:start_on) %><br /><br />
     <strong>End On: </strong><%= form.date_field(:end_on) %><br /><br />
     <strong>Instructor:</strong> <%= form.select :instructor_id, options_for_select(@instructors, @cohort.instructor_id) %><br /><br />
     <strong>Course:</strong> <%= form.select :course_id, options_for_select(@courses, @cohort.course_id) %><br /><br />
     <div><strong> Student: </strong></div>
     <%= form.select :student_id, options_for_select(@students, @cohort.student_id) %><br /><br />
<%= form.submit :class => "btn-primary btn" %> 
     <% end %>
    </div>
</div>
</div>
</div>
</div>



 
     