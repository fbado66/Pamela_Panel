<h1>Instructors#new</h1>
<p>Find me in app/views/instructors/new.html.erb</p>

<div class="col-6 offset-md-3">
 <div class="jumbotron">
   <div class="col-md-3"> 
    <div class="form-group">
<%= form_with(model: @instructor) do |form| %>
  First Name: <%= form.text_field(:first_name) %><br /><br/>
  Last Name: <%= form.text_field(:last_name) %><br /><br/>
  Age: <%= form.text_field(:age) %><br /><br/>
  Salary: <%= form.text_field(:salary) %><br /><br/>
  Education:<%= form.select(:education, options_for_select([['High School'], ['College'], ['Masters'], ['PHD']]), { include_blank: true }) %> <br /><br/>
  Email: <%= form.text_field(:email) %><br /><br/>
  Cohort: <%= form.select :cohort_id, options_for_select(@cohorts, @instructor.id) %><br /><br />
<%= form.submit class: "btn btn-primary"%>
<% end %>
</div>
</div>
</div>
</div>