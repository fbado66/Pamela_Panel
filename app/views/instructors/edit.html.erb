<h1>Instructors#edit</h1>
<p>Find me in app/views/instructors/edit.html.erb</p>

<%= form_with(model: @instructor) do |form| %>
  First Name: <%= form.text_field(:first_name) %><br />
  Last Name: <%= form.text_field(:last_name) %><br />
  Age: <%= form.text_field(:age) %><br />
  Salary: <%= form.text_field(:salary) %><br />
  Education:<%= form.select(:education, options_for_select([['High School'], ['College'], ['Masters'], ['PHD']]), { include_blank: true }) %> <br />
  Email: <%= form.text_field(:email) %><br />
  Cohort: <%= form.select :cohort_id, options_for_select(@cohorts, @instructor.id) %><br />
<%= form.submit %>
<% end %>